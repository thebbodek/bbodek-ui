    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "ngrok": "ngrok http 6006 --domain=bbodek.ngrok.io",
    "format": "prettier --write \"**/*.{ts,tsx,css,json,md}\""
